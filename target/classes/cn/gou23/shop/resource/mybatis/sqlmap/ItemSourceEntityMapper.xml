	<!-- 黑名单查询 -->
	<select id="selectBlacklist" resultMap="BaseResultMap">
		SELECT item_source.* FROM item_source INNER JOIN blacklist
		ON item_source.TITLE LIKE CONCAT('%',blacklist.word,'%')
		WHERE item_source.sale_status!='下架'
	</select>

	<!-- 货源黑名单 -->
	<select id="selectSourceBlacklist" resultMap="BaseResultMap">
		SELECT item_source.* FROM item_source INNER JOIN source_blacklist
		ON item_source.TYPE = source_blacklist.TYPE AND
		source_blacklist.SOURCE_OWNER = item_source.SOURCE_OWNER
		WHERE item_source.sale_status!='下架'
	</select>
